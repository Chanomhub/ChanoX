{
  "name": "chanomhub",
  "private": true,
  "version": "0.9.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:build:x64": "tauri build --target x86_64-pc-windows-msvc",
    "tauri:build:x32": "tauri build --target i686-pc-windows-msvc",
    "tauri:build:linux:x64": "tauri build --target x86_64-unknown-linux-gnu",
    "tauri:build:linux:arm64": "tauri build --target aarch64-unknown-linux-gnu",
    "tauri:build:mac:x64": "tauri build --target x86_64-apple-darwin",
    "tauri:build:mac:arm64": "tauri build --target aarch64-apple-darwin",
    "tauri:build:all": "bun tauri:build:x64 && pnpm tauri:build:x32",
    "date:windows": "powershell -Command \"[System.DateTime]::UtcNow.ToString('yyyy-MM-ddTHH:mm:ssZ')\"",
    "date:linux": "date -u +\"%Y-%m-%dT%H:%M:%SZ\""
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@tabler/icons-react": "^3.34.0",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/vite": "^4.1.10",
    "@tauri-apps/api": "^2.5.0",
    "@tauri-apps/plugin-dialog": "^2.2.2",
    "@tauri-apps/plugin-fs": "^2.3.0",
    "@tauri-apps/plugin-opener": "^2.2.7",
    "@tauri-apps/plugin-os": "^2.2.1",
    "@tauri-apps/plugin-shell": "^2.2.1",
    "@tauri-apps/plugin-updater": "^2.7.1",
    "@tiptap/starter-kit": "^2.14.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.15.31",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.2.6",
    "framer-motion": "^12.17.3",
    "install": "^0.13.0",
    "lucide-react": "^0.487.0",
    "postcss-cli": "^11.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.2",
    "std-env": "^3.9.0",
    "tailwind-merge": "^3.3.1",
    "vite-plugin-solid": "^2.11.6",
    "vite-plugin-tauri": "^4.0.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.5.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.0.43",
    "i": "^0.3.7",
    "postcss": "^8.5.5",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}
